param workflows_NotificationsMissingWUDevicesReport_name string
param connections_excelonlinebusiness_name string = 'excelonlinebusiness'
param connections_office365_name string = 'office365'
param connections_SharePoint_name string = 'SharePointOnline'
param userAssignedIdentities_NotificationsMissingWUDevicesReport_Identity_name string = 'NotificationsMissingWUDevicesReport-ManagedIdentity'
param resourceLocation string

resource workflows_NotificationsMissingWUDevicesReport_name_resource 'Microsoft.Logic/workflows@2019-05-01' = {
  name: workflows_NotificationsMissingWUDevicesReport_name
  location: resourceLocation
  identity: {
    type: 'UserAssigned'
    userAssignedIdentities: {
      '${resourceId('Microsoft.ManagedIdentity/userAssignedIdentities/',userAssignedIdentities_NotificationsMissingWUDevicesReport_Identity_name)}': {}
    }
  }
  properties: {
    state: 'Enabled'
    definition: {
      '$schema': 'https://schema.management.azure.com/providers/Microsoft.Logic/schemas/2016-06-01/workflowdefinition.json#'
      contentVersion: '1.0.0.0'
      parameters: {
        '$connections': {
          defaultValue: {}
          type: 'Object'
        }
      }
      triggers: {
        Recurrence: {
          recurrence: {
            frequency: 'Week'
            interval: 1
            schedule: {
              weekDays: [
                'Monday'
              ]
            }
          }
          evaluatedRecurrence: {
            frequency: 'Week'
            interval: 1
            schedule: {
              weekDays: [
                'Monday'
              ]
            }
          }
          type: 'Recurrence'
        }
      }
      actions: {
        Compose_Excel_file: {
          runAfter: {
            Initialize_variable_WorkspaceID: [
              'Succeeded'
            ]
          }
          type: 'Compose'
          inputs: {
            '$content': ''
            '$content-type': 'application/vnd.openxmlformats-officedocument.spreadsheetml.sheet'
          }
        }
        Create_Excel_file: {
          runAfter: {
            Compose_Excel_file: [
              'Succeeded'
            ]
          }
          type: 'ApiConnection'
          inputs: {
            body: '@outputs(\'Compose_Excel_file\')'
            host: {
              connection: {
                name: '@parameters(\'$connections\')[\'sharepointonline\'][\'connectionId\']'
              }
            }
            method: 'post'
            path: '/datasets/@{encodeURIComponent(encodeURIComponent(\'\'))}/files'
            queries: {
              folderPath: '/'
              name: 'MissingDevicesWUReport@{formatDateTime(utcNow(), \'yyyy-MM-dd\')}.xlsx'
              queryParametersSingleEncoded: true
            }
          }
          runtimeConfiguration: {
            contentTransfer: {
              transferMode: 'Chunked'
            }
          }
        }
        Create_table: {
          runAfter: {
            Create_Excel_file: [
              'Succeeded'
            ]
          }
          type: 'ApiConnection'
          inputs: {
            body: {
              ColumnsNames: 'displayName,deviceId,startTime'
              Range: 'A1:C1'
              TableName: 'MissingDevices'
            }
            host: {
              connection: {
                name: '@parameters(\'$connections\')[\'excelonlinebusiness\'][\'connectionId\']'
              }
            }
            method: 'post'
            path: '/drives/@{encodeURIComponent(\'\')}/files/@{encodeURIComponent(encodeURIComponent(body(\'Create_Excel_file\')?[\'Id\']))}/tables'
            queries: {
              source: ''
            }
          }
        }
        Delay: {
          runAfter: {
            For_each_missing_device: [
              'Succeeded'
            ]
          }
          type: 'Wait'
          inputs: {
            interval: {
              count: 2
              unit: 'Minute'
            }
          }
        }
        For_each_missing_device: {
          foreach: '@variables(\'DeviceResultsHelper\')'
          actions: {
            Compose_Device_details: {
              runAfter: {
                Compose_loop_over_all_devices: [
                  'Succeeded'
                ]
              }
              type: 'Compose'
              inputs: {
                deviceId: '@outputs(\'Compose_loop_over_all_devices\')[1]'
                startTime: '@outputs(\'Compose_loop_over_all_devices\')[4]'
              }
            }
            Compose_loop_over_all_devices: {
              runAfter: {}
              type: 'Compose'
              inputs: '@items(\'For_each_missing_device\')'
            }
            For_each_Entra_Device: {
              foreach: '@body(\'Parse_JSON_GET_Devices\')?[\'value\']'
              actions: {
                Add_a_row_into_a_table: {
                  runAfter: {
                    Compose_Table_row: [
                      'Succeeded'
                    ]
                  }
                  type: 'ApiConnection'
                  inputs: {
                    body: '@outputs(\'Compose_Table_row\')'
                    host: {
                      connection: {
                        name: '@parameters(\'$connections\')[\'excelonlinebusiness\'][\'connectionId\']'
                      }
                    }
                    method: 'post'
                    path: '/codeless/v1.2/drives/@{encodeURIComponent(\'\')}/items/@{encodeURIComponent(encodeURIComponent(body(\'Create_Excel_file\')?[\'Id\']))}/workbook/tables/@{encodeURIComponent(\'MissingDevices\')}/rows'
                    queries: {
                      source: ''
                    }
                  }
                }
                Compose_Table_row: {
                  runAfter: {}
                  type: 'Compose'
                  inputs: {
                    deviceId: '@body(\'Parse_JSON_Device_details\')?[\'deviceId\']'
                    displayName: '@items(\'For_each_Entra_Device\')?[\'displayName\']'
                    startTime: '@body(\'Parse_JSON_Device_details\')?[\'startTime\']'
                  }
                }
              }
              runAfter: {
                Parse_JSON_GET_Devices: [
                  'Succeeded'
                ]
              }
              type: 'Foreach'
            }
            HTTP_GET_Devices: {
              runAfter: {
                Parse_JSON_Device_details: [
                  'Succeeded'
                ]
              }
              type: 'Http'
              inputs: {
                authentication: {
                  audience: 'https://graph.microsoft.com'
                  identity: resourceId('Microsoft.ManagedIdentity/userAssignedIdentities', userAssignedIdentities_NotificationsMissingWUDevicesReport_Identity_name)
                  type: 'ManagedServiceIdentity'
                }
                method: 'GET'
                uri: 'https://graph.microsoft.com/v1.0/devices?$filter=(deviceId eq \'@{body(\'Parse_JSON_Device_details\')?[\'deviceId\']}\')'
              }
            }
            Parse_JSON_Device_details: {
              runAfter: {
                Compose_Device_details: [
                  'Succeeded'
                ]
              }
              type: 'ParseJson'
              inputs: {
                content: '@outputs(\'Compose_Device_details\')'
                schema: {
                  properties: {
                    deviceId: {
                      type: 'string'
                    }
                    startTime: {
                      type: 'string'
                    }
                  }
                  type: 'object'
                }
              }
            }
            Parse_JSON_GET_Devices: {
              runAfter: {
                HTTP_GET_Devices: [
                  'Succeeded'
                ]
              }
              type: 'ParseJson'
              inputs: {
                content: '@body(\'HTTP_GET_Devices\')'
                schema: {
                  properties: {
                    '@@microsoft.graph.tips': {
                      type: 'string'
                    }
                    '@@odata.context': {
                      type: 'string'
                    }
                    value: {
                      items: {
                        properties: {
                          accountEnabled: {
                            type: 'boolean'
                          }
                          alternativeSecurityIds: {
                            items: {
                              properties: {
                                identityProvider: {}
                                key: {
                                  type: 'string'
                                }
                                type: {
                                  type: 'integer'
                                }
                              }
                              required: [
                                'type'
                                'identityProvider'
                                'key'
                              ]
                              type: 'object'
                            }
                            type: 'array'
                          }
                          approximateLastSignInDateTime: {
                            type: 'string'
                          }
                          complianceExpirationDateTime: {}
                          createdDateTime: {
                            type: 'string'
                          }
                          deletedDateTime: {}
                          deviceCategory: {}
                          deviceId: {
                            type: 'string'
                          }
                          deviceMetadata: {}
                          deviceOwnership: {
                            type: 'string'
                          }
                          deviceVersion: {
                            type: 'integer'
                          }
                          displayName: {
                            type: 'string'
                          }
                          domainName: {}
                          enrollmentProfileName: {}
                          enrollmentType: {
                            type: 'string'
                          }
                          extensionAttributes: {
                            properties: {
                              extensionAttribute1: {}
                              extensionAttribute10: {}
                              extensionAttribute11: {}
                              extensionAttribute12: {}
                              extensionAttribute13: {}
                              extensionAttribute14: {}
                              extensionAttribute15: {}
                              extensionAttribute2: {}
                              extensionAttribute3: {}
                              extensionAttribute4: {}
                              extensionAttribute5: {}
                              extensionAttribute6: {}
                              extensionAttribute7: {}
                              extensionAttribute8: {}
                              extensionAttribute9: {}
                            }
                            type: 'object'
                          }
                          externalSourceName: {}
                          id: {
                            type: 'string'
                          }
                          isCompliant: {
                            type: 'boolean'
                          }
                          isManaged: {
                            type: 'boolean'
                          }
                          isRooted: {
                            type: 'boolean'
                          }
                          managementType: {
                            type: 'string'
                          }
                          manufacturer: {
                            type: 'string'
                          }
                          mdmAppId: {
                            type: 'string'
                          }
                          model: {
                            type: 'string'
                          }
                          onPremisesLastSyncDateTime: {}
                          onPremisesSyncEnabled: {}
                          operatingSystem: {
                            type: 'string'
                          }
                          operatingSystemVersion: {
                            type: 'string'
                          }
                          physicalIds: {
                            items: {
                              type: 'string'
                            }
                            type: 'array'
                          }
                          profileType: {
                            type: 'string'
                          }
                          registrationDateTime: {
                            type: 'string'
                          }
                          sourceType: {}
                          systemLabels: {
                            type: 'array'
                          }
                          trustType: {
                            type: 'string'
                          }
                        }
                        required: [
                          'id'
                          'deletedDateTime'
                          'accountEnabled'
                          'approximateLastSignInDateTime'
                          'complianceExpirationDateTime'
                          'createdDateTime'
                          'deviceCategory'
                          'deviceId'
                          'deviceMetadata'
                          'deviceOwnership'
                          'deviceVersion'
                          'displayName'
                          'domainName'
                          'enrollmentProfileName'
                          'enrollmentType'
                          'externalSourceName'
                          'isCompliant'
                          'isManaged'
                          'isRooted'
                          'managementType'
                          'manufacturer'
                          'mdmAppId'
                          'model'
                          'onPremisesLastSyncDateTime'
                          'onPremisesSyncEnabled'
                          'operatingSystem'
                          'operatingSystemVersion'
                          'physicalIds'
                          'profileType'
                          'registrationDateTime'
                          'sourceType'
                          'systemLabels'
                          'trustType'
                          'extensionAttributes'
                          'alternativeSecurityIds'
                        ]
                        type: 'object'
                      }
                      type: 'array'
                    }
                  }
                  type: 'object'
                }
              }
            }
          }
          runAfter: {
            Initialize_variable_DeviceResultsHelper: [
              'Succeeded'
            ]
          }
          type: 'Foreach'
        }
        Get_file_content: {
          runAfter: {
            Delay: [
              'Succeeded'
            ]
          }
          type: 'ApiConnection'
          inputs: {
            host: {
              connection: {
                name: '@parameters(\'$connections\')[\'sharepointonline\'][\'connectionId\']'
              }
            }
            method: 'get'
            path: '/datasets/@{encodeURIComponent(encodeURIComponent(\'\'))}/files/@{encodeURIComponent(body(\'Create_Excel_file\')?[\'Id\'])}/content'
            queries: {
              inferContentType: true
            }
          }
        }
        Initialize_variable_DeviceResults: {
          runAfter: {
            _HTTP_GET_Missing_devices: [
              'Succeeded'
            ]
          }
          type: 'InitializeVariable'
          inputs: {
            variables: [
              {
                name: 'DeviceResults'
                type: 'object'
                value: '@body(\'_HTTP_GET_Missing_devices\')'
              }
            ]
          }
        }
        Initialize_variable_DeviceResultsHelper: {
          runAfter: {
            Initialize_variable_DeviceResults: [
              'Succeeded'
            ]
          }
          type: 'InitializeVariable'
          inputs: {
            variables: [
              {
                name: 'DeviceResultsHelper'
                type: 'array'
                value: '@variables(\'DeviceResults\')?[\'tables\'][0]?[\'rows\']'
              }
            ]
          }
        }
        Initialize_variable_WorkspaceID: {
          runAfter: {}
          type: 'InitializeVariable'
          inputs: {
            variables: [
              {
                name: 'WorkspaceID'
                type: 'string'
                value: ''
              }
            ]
          }
        }
        'Send_an_email_from_a_shared_mailbox_(V2)': {
          runAfter: {
            Get_file_content: [
              'Succeeded'
            ]
          }
          type: 'ApiConnection'
          inputs: {
            body: {
              Attachments: [
                {
                  ContentBytes: '@{base64(body(\'Get_file_content\'))}'
                  Name: '@body(\'Create_Excel_file\')?[\'Name\']'
                }
              ]
              Body: '<p>Hi IT Hero,<br>\n<br>\nHere is your weekly report of missing devices in Windows Update for Business.<br>\n<br>\nRegards,<br>\n<br>\nYour automation account<br>\nInTheCloud247.com</p>'
              Importance: 'Normal'
              MailboxAddress: ''
              Subject: 'Weekly report of missing devices in Windows Update for Business'
              To: ''
            }
            host: {
              connection: {
                name: '@parameters(\'$connections\')[\'office365\'][\'connectionId\']'
              }
            }
            method: 'post'
            path: '/v2/SharedMailbox/Mail'
          }
        }
        _HTTP_GET_Missing_devices: {
          runAfter: {
            Create_table: [
              'Succeeded'
            ]
          }
          type: 'Http'
          inputs: {
            authentication: {
              audience: 'https://api.loganalytics.io'
              identity: resourceId('Microsoft.ManagedIdentity/userAssignedIdentities', userAssignedIdentities_NotificationsMissingWUDevicesReport_Identity_name)
              type: 'ManagedServiceIdentity'
            }
            method: 'GET'
            queries: {
              query: 'UCDeviceAlert\n| where AlertSubtype == "DeviceDiagnosticDataNotReceived"\n| where TimeGenerated > ago(2d)\n| project DeviceName, AzureADDeviceId, AlertStatus, AlertSubtype, StartTime, AlertData, Description, Recommendation'
            }
            uri: 'https://api.loganalytics.io/v1/workspaces/@{variables(\'WorkspaceID\')}/query'
          }
        }
      }
      outputs: {}
    }
    parameters: {
      '$connections': {
        value: {
          excelonlinebusiness: {
            connectionId: resourceId('Microsoft.Web/connections', connections_excelonlinebusiness_name)
            connectionName: 'excelonlinebusiness'
            id: '${subscription().id}/providers/Microsoft.Web/locations/${resourceLocation}/managedApis/excelonlinebusiness'
          }
          office365: {
            connectionId: resourceId('Microsoft.Web/connections', connections_office365_name)
            connectionName: 'office365'
            id: '${subscription().id}/providers/Microsoft.Web/locations/${resourceLocation}/managedApis/office365'
          }
          sharepointonline: {
            connectionId: resourceId('Microsoft.Web/connections', connections_SharePoint_name)
            connectionName: 'SharePointOnline'
            id: '${subscription().id}/providers/Microsoft.Web/locations/${resourceLocation}/managedApis/SharePointOnline'
          }
        }
      }
    }
  }
}
